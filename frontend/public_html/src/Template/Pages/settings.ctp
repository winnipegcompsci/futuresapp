<div class="row">
    <div class="columns col-lg-12 col-md-6">
        <div class="panel panel-success">
            <div class="panel-heading">
                Trading Advisor
            </div>
            <div class="panel-body">
                
                <form class="form-horizontal">
                    <fieldset>

                    <!-- Multiple Radios -->
                    <div class="form-group">
                      <label class="col-md-4 control-label" for="enabled">Perform Trades Automatically </label>
                      <div class="col-md-4">
                      <div class="radio">
                        <label for="enabled-0">
                          <input type="radio" name="enabled" id="enabled-0" value="1" checked="checked">
                          Enabled
                        </label>
                        </div>
