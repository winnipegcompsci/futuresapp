                    <!-- Select Basic -->
                    <div class="form-group">
                      <label class="col-md-4 control-label" for="tradingstrategy">Trading Strategy</label>
                      <div class="col-md-4">
                        <select id="tradingstrategy" name="tradingstrategy" class="form-control">
                          <option value="1">DEMA</option>
                          <option value="2">MACD</option>
                          <option value="3">PPO</option>
                          <option value="4">RSI</option>
                        </select>
                      </div>
                    </div>

                    </fieldset>
                </form>

            </div>
        </div>
    </div>
</div>

